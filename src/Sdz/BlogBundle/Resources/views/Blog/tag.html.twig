{# src/Sdz/BlogBundle/Resources/views/Blog/index.html.twig #}

{% extends "SdzBlogBundle::layout.html.twig" %}

{% block title %}
	Tag - {{ parent() }}
{% endblock %}

{% block sdzblog_body %}

	<h2>Liste des tags</h2>

	<ul>
		{% for tag in tags %}
    {{ tag.nom }}<br />
		{% endfor %}
	</ul>

{% endblock %}
